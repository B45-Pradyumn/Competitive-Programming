class Solution {
    public String toLowerCase(String s) {

        char[] chararr = s.toCharArray();

        for(int i=0; i<chararr.length; i++){
            if(chararr[i]>='A' && chararr[i]<='Z'){
                chararr[i] = (char)(chararr[i]+32);
            }
        }

        return new String(chararr);
        
    }
}
